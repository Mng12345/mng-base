{
  "name": "mng-base",
  "version": "1.2.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mng12345/mng-base.git"
  },
  "homepage": "https://github.com/Mng12345/mng-base",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./option": {
      "types": "./dist/esm/option.d.ts",
      "import": "./dist/esm/option.js",
      "require": "./dist/cjs/option.js"
    },
    "./result": {
      "types": "./dist/esm/result.d.ts",
      "import": "./dist/esm/result.js",
      "require": "./dist/cjs/result.js"
    },
    "./types": {
      "types": "./dist/esm/types.d.ts"
    }
  },
  "scripts": {
    "build:cjs": "yarn test & tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build": "rimraf dist & yarn build:cjs & yarn build:esm",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.2"
  }
}
